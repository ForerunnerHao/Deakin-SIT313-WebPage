<script>
    // CSS_Animation
    window.onload = function () {
        const animate_01 = document.getElementById("animate_01");
        const animate_02 = document.getElementById("animate_02");
        animate_02.style.display = "none";
    }
    animate_01.addEventListener('animationend', () => {
        if (animate_01.classList.contains("animate__fadeIn")) {

            animate_01.classList.remove("animate__fadeIn");
            animate_01.classList.add("animate__fadeOut");

        } else if (animate_01.classList.contains("animate__fadeOut")) {
            animate_01.style.display = "none";
            animate_02.style.display = "block";
        }
    })
</script>


